{
  "name": "jenkins-plugin-site",
  "version": "0.0.1",
  "description": "Boilerplate for kick starting a React Project with ES6 (Babel) and Hot reloader using Webpack.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --port 5000 --inline --hot --progress --colors --display-error-details --history-api-fallback",
    "deploy": "NODE_ENV=production webpack --config webpack.config.js --progress --colors --display-error-details",
    "lint": "eslint app",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha",
    "test:watch": "mocha --watch --reporter min",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jenkins-infra/plugin-site.git"
  },
  "keywords": [
    "jenkins-ci",
    "plugins",
    "es6",
    "babel",
    "webpack"
  ],
